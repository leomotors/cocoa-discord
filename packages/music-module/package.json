{
  "name": "@cocoa-discord/music-module",
  "version": "3.0.0",
  "description": "discord.js music bot that can be run standalone or as a module for other bots",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/leomotors/cocoa-discord"
  },
  "author": "leomotors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm lm-config && tsc",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test": "vitest run && node tests/index.js",
    "test:cov": "vitest run --coverage",
    "bot": "pnpm build && node tests/bot.js",
    "prepack": "rm -rf dist/* && pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.10.0",
    "@leomotors/scripts": "^3.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "libsodium-wrappers": "^0.7.15",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "dependencies": {
    "@discordjs/voice": "^0.19.0",
    "@distube/ytdl-core": "^4.16.12",
    "chalk": "^5.6.2",
    "cocoa-discord": "^3.2.0",
    "discord.js": "^14.24.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.46.0",
    "play-dl": "^1.9.7",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0"
  },
  "keywords": [
    "discord-bot",
    "music-bot",
    "discordjs",
    "cocoa-discord"
  ]
}
