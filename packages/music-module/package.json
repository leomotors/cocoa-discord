{
  "name": "@leomotors/music-bot",
  "version": "1.0.0-beta.0",
  "description": "discord.js music bot that can be run standalone or as a module for other bots",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leomotors/cocoa-discord"
  },
  "author": "Leomotors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@8.7.6",
  "scripts": {
    "build": "pnpm lm-config && tsc",
    "lint": "eslint src",
    "format": "eslint src --fix",
    "test": "node tests/index.js",
    "bot": "pnpm build && node tests/bot.js",
    "prepack": "rimraf dist/* && pnpm build && pnpm lint && pnpm test"
  },
  "devDependencies": {
    "@discordjs/opus": "0.9.0",
    "@leomotors/scripts": "3.1.0",
    "@trivago/prettier-plugin-sort-imports": "4.2.0",
    "@types/node": "18.17.17",
    "@types/uuid": "9.0.4",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "dotenv": "16.3.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "libsodium-wrappers": "0.7.13",
    "prettier": "3.0.3",
    "rimraf": "5.0.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@discordjs/voice": "^0.16.0",
    "chalk": "^5.3.0",
    "cocoa-discord-utils": "^2.3.0",
    "discord.js": "^14.13.0",
    "play-dl": "^1.9.6",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1"
  },
  "keywords": [
    "discord-bot",
    "music-bot",
    "discordjs",
    "cocoa-discord"
  ]
}
